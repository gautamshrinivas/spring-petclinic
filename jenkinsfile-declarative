pipeline {
    agent any
    stages {
        stage('test') {
            steps {
                sh 'echo this is declarative pipeline'
            }
        }
        stage('learning') {
            steps {
                git url: 'https://github.com/gautamshrinivas/spring-petclinic.git', 
                    branch: 'main'
            }
        }
    }
}
